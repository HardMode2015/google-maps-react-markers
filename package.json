{
	"name": "google-maps-react-markers",
	"version": "1.2.0",
	"description": "Google Maps library that accepts markers as react components",
	"author": "Giorgia Bosello",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/giorgiabosello/google-maps-react-markers.git"
	},
	"homepage": "https://giorgiabosello.github.io/google-maps-react-markers/",
	"main": "dist/index.js",
	"module": "dist/index.modern.js",
	"source": "src/index.js",
	"keywords": [
		"react",
		"google-maps",
		"markers",
		"map",
		"google",
		"maps",
		"google-maps-react",
		"google-maps-react-markers",
		"react-component"
	],
	"scripts": {
		"build": "microbundle-crl --no-compress --format modern,cjs",
		"dev": "microbundle-crl watch --no-compress --format modern,cjs",
		"predeploy": "cd docs && pnpm install && pnpm build",
		"deploy": "gh-pages -d docs/build",
		"publish": "pnpm build && npm publish"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"webpack": "^5.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.0",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-modules": "^0.1.5",
		"@babel/preset-react": "^7.18.6",
		"babel-loader": "^9.1.2",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"gh-pages": "^5.0.0",
		"microbundle-crl": "^0.13.10",
		"prettier": "^2.8.3",
		"react-scripts": "^5.0.1",
		"typescript": "^4.9.5"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@babel/plugin-syntax-flow": "^7.18.6",
		"@babel/plugin-transform-react-jsx": "^7.20.13",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"engines": {
		"node": ">=16",
		"npm": ">=8",
		"pnpm": ">=7",
		"yarn": ">=1"
	}
}
